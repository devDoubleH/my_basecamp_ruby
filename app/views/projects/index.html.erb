  <main>
    <div class="container">
      <div class="row my-3 justify-content-center">
            <h2 class="centered">Projects</h2>
      </div>
      <div class="projects-filter row justify-content-around">
          <div id="all" class="col-3 btn btn btn-outline-secondary btn-sm projects-filter-btn">
            <%= svg 'all' %>All projects</div>
          <div id="owner" class="col-3 btn btn btn-outline-secondary btn-sm projects-filter-btn">
            <%= svg 'creator' %>Created by me</div>
          <div id="member" class="col-3 btn btn btn-outline-secondary btn-sm projects-filter-btn">
            <%= svg 'member' %>Shared with me</div>
      </div>
      <div class="row mt-5">
        <%if @projects %>
          <%= render :partial => 'list', :object => @projects, as: 'projects' %>
        <% end %>
      </div>
    </div>
</main>
